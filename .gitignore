# Ignore bundler config
/.bundle

# Include SimpleCov directory
/coverage

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore .rvmrc
.rvmrc

# Ignore .env
.env

# Ignore secret token
/config/app_environment_variables.rb